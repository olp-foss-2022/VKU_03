<!-- BEGIN: main  -->
<div style="
        position: relative;
        background-image: url('{DATA.file_image}');
        /* Set a specific height */
        min-height: 200px;
        width: 100%;
        /* Create the parallax scrolling effect */
        background-attachment: fixed;
        background-position: center;
        background-repeat: no-repeat;
        background-size: contain;">
    <h1 class="text-center" style="position: absolute; left: 50%; top: 50%; transform: translate(-50%, -50%)">{DATA.des}</h1>
</div>
<!-- END: main -->
